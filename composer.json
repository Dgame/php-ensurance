{
    "name": "dgame/php-ensurance",
    "description": "php ensurance",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-ensurance",
    "keywords": [
        "Ensurance",
        "Enforcement",
        "Assertion",
        "Design by contract"
    ],
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-ensurance"
        }
    ],
    "require": {
        "php": "^7.1",
        "dgame/php-type": "^0.9"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Ensurance\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpstan/phpstan": "^0.11.2"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
